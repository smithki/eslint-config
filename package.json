{
  "name": "@ikscodes/eslint-config",
  "version": "8.1.0",
  "description": "An opinionated ESLint config with Airbnb JavaScript, TypeScript, React, and Prettier baked-in.",
  "author": "Ian K Smith <smithki707@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "javascript.js",
    "typescript.js",
    "rules"
  ],
  "repository": "https://github.com/smithki/eslint-config",
  "bugs": "https://github.com/smithki/eslint-config/issues",
  "keywords": [
    "eslint",
    "airbnb",
    "prettier",
    "typescript",
    "react"
  ],
  "scripts": {
    "lint": "eslint --fix ./**/*.js"
  },
  "dependencies": {
    "@ikscodes/prettier-config": "~2.0.1",
    "@rushstack/eslint-patch": "~1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0"
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "husky": "^4.3.0",
    "lint-staged": "^9.5.0"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "prettier": ">=2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
